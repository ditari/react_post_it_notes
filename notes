CREATE TABLE post_it_notes (
    id INT AUTO_INCREMENT PRIMARY KEY,
    title VARCHAR(255),=
    notes TEXT
);

INSERT INTO post_it_notes (title, notes) 
VALUES ('title 1', 'notes 1');

